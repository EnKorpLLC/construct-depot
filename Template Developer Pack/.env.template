# Application
NODE_ENV=development
PORT=3000
HOST=localhost

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=project_db
DB_USER=db_user
DB_PASSWORD=your_password

# Authentication
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=24h
API_KEY=your_api_key

# External Services
API_URL=https://api.example.com
WEBHOOK_URL=https://webhook.example.com

# Logging
LOG_LEVEL=debug
LOG_FILE=logs/app.log

# Security
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100

# AI Development
AI_LOG_LEVEL=debug
AI_LOG_DIR=.ai-logs
TEMPLATE_VERSION=1.0.0

# Feature Flags
ENABLE_FEATURE_X=false
ENABLE_FEATURE_Y=false

# Documentation
DOC_BASE_URL=http://localhost:3000/docs
API_DOC_URL=http://localhost:3000/api-docs

# Testing
TEST_DB_NAME=test_db
TEST_API_KEY=test_key

# Monitoring
METRICS_PORT=9090
HEALTH_CHECK_PATH=/health 